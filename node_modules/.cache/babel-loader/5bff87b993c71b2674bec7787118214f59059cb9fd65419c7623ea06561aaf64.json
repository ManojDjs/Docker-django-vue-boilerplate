       },\r\n          },\r\n          legend: {\r\n            labels: {\r\n              color: \"#495057\",\r\n              font: {\r\n                size: 24,\r\n              },\r\n            },\r\n          },\r\n        },\r\n        scales: {\r\n          x: {\r\n            ticks: {\r\n              color: \"#495057\",\r\n              callback: function (value, index, ticks) {\r\n                return \"₹\" + value;\r\n              },\r\n              scaleLabel: {\r\n                display: true,\r\n              },\r\n            },\r\n            grid: {\r\n              color: \"#591BDF\",\r\n            },\r\n          },\r\n          y: {\r\n            ticks: {\r\n              color: \"#495057\",\r\n              //   mirror: true, //Show y-axis labels inside horizontal bars\r\n            },\r\n            grid: {\r\n              color: \"#ebedef\",\r\n            },\r\n            gridLines: {\r\n              drawOnChartArea: false,\r\n            },\r\n          },\r\n        },\r\n      },\r\n      basicData: {\r\n        labels: [\r\n          \"January\",\r\n          \"February\",\r\n          \"March\",\r\n          \"April\",\r\n          \"May\",\r\n          \"June\",\r\n          \"July\",\r\n        ],\r\n        datasets: [\r\n          {\r\n            label: \"My First dataset\",\r\n            backgroundColor: \"#DF711B\",\r\n            barThickness: 25,\r\n            padding: {\r\n              top: 5,\r\n            },\r\n            data: [650, 59, 80, 81, 56, 55, 40],\r\n          },\r\n          //   {\r\n          //     label: \"Line Dataset\",\r\n          //     backgroundColor: \"#42A5F5\",\r\n          //     data: [500, 500, 500, 500, 500, 500, 500, 500],\r\n          //     type: \"line\",\r\n          //     // this dataset is drawn on top\r\n          //     order: 1,\r\n          //   },\r\n        ],\r\n      },\r\n    };\r\n  },\r\n  components: {\r\n    Nav,\r\n    Footer,\r\n  },\r\n  computed: {\r\n  