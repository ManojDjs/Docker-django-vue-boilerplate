             <div class=\"mr-5 mt-3\">\r\n                <span class=\"font-medium text-500\">USER NAME</span>\r\n                <div class=\"text-yellow-700 mt-2\">{{ username }}</div>\r\n              </div>\r\n              <div class=\"mr-5 mt-3\">\r\n                <span class=\"font-medium text-500\">PROJECTS</span>\r\n                <div class=\"text-700 mt-2\">26</div>\r\n              </div>\r\n              <div class=\"mr-5 mt-3\">\r\n                <span class=\"font-medium text-500\">ACTIVE</span>\r\n                <div class=\"text-700 mt-2\" v-if=\"is_active\">\r\n                  <i class=\"pi pi-check\" style=\"font-size: 1rem\"></i>\r\n                </div>\r\n                <div class=\"text-700 mt-2\" v-else>\r\n                  <i class=\"pi pi-times\" style=\"font-size: 1rem\"></i>\r\n                </div>\r\n              </div>\r\n              <div class=\"mt-3\">\r\n                <span class=\"font-medium text-500\">Email</span>\r\n                <div class=\"text-700 mt-2\">{{ email }}</div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div\r\n          class=\"flex align-items-center justify-content-center w-4rem h-4rem bg-indigo-500 font-bold text-white border-round m-2\"\r\n        >\r\n          2\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"grid\">\r\n      <div class=\"col-12 md:col-6 lg:col-6\">\r\n        <div\r\n          class=\"flex align-items-center justify-content-center bg-white px-9 py-5\"\r\n        >\r\n          <div\r\n            class=\"flex card-container white-container align-items-start flex-column lg:flex-row lg:justify-content-between\"\r\n          >\r\n            <div class=\"flex align-items-start flex-column md:flex-row\">\r\n              <div class=\"flex align-items-start flex-column md:flex-row\">\r\n                <img\r\n                  :src=\"Image\"\r\n                  class=\"mr-5 mb-5 lg:mb-5 md:m-5\"\r\n                  style=\"width: 90px; height: 90px\"\r\n                />\r\n              </div>\r\n              <div>\r\n                <span class=\"uppercase text-indigo-900 font-medium text-3xl\"\r\n                  >{{ first_name }} {{ last_name }}</span\r\n                ><i class=\"pi pi-star text-2xl ml-4 text-yellow-500\"></i>\r\n                <div class=\"flex align-items-center flex-wrap text-sm\">\r\n                  <div class=\"mr-5 mt-3\">\r\n                    <span class=\"font-medium text-500\">USER NAME</span>\r\n                    <div class=\"text-yellow-700 mt-2\">{{ username }}</div>\r\n                  </div>\r\n                  <div class=\"mr-5 mt-3\">\r\n                    <span class=\"font-medium text-500\">PROJECTS</span>\r\n                    <div class=\"text-700 mt-2\">26</div>\r\n                  </div>\r\n                  <div class=\"mr-5 mt-3\">\r\n                    <span class=\"font-medium text-500\">ACTIVE</span>\r\n                    <div class=\"text-700 mt-2\" v-if=\"is_active\">\r\n                      <i class=\"pi pi-check\" style=\"font-size: 1rem\"></i>\r\n                    </div>\r\n                    <div class=\"text-700 mt-2\" v-else>\r\n                      <i class=\"pi pi-times\" style=\"font-size: 1rem\"></i>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"mt-3\">\r\n                    <span class=\"font-medium text-500\">Email</span>\r\n                    <div class=\"text-700 mt-2\">{{ email }}</div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-12 md:col-6 lg:col-6\">\r\n        <div class=\"p-5 flex flex-column flex-auto\">\r\n          <div>\r\n            <div class=\"uppercase text-yellow-500 font-medium text-xl mb-3\">\r\n              Profile\r\n            </div>\r\n            <div class=\"surface-card p-4 shadow-2 border-round\">\r\n              <div class=\"grid formgrid p-fluid\">\r\n                <div class=\"field mb-4 col-12\">\r\n                  <label for=\"nickname\" class=\"font-medium\">User Name</label\r\n                  ><input\r\n                    class=\"p-inputtext p-component\"\r\n                    id=\"nickname\"\r\n                    type=\"text\"\r\n                    v-model=\"username\"\r\n                  />\r\n                </div>\r\n                <div class=\"surface-100 mb-3 col-12\" style=\"height: 2px\"></div>\r\n                <div class=\"field mb-4 col-12 md:col-6\">\r\n                  <label for=\"bio\" class=\"font-medium\">First Name</label\r\n                  ><input\r\n                    class=\"p-inputtext p-component\"\r\n                    id=\"nickname\"\r\n                    type=\"text\"\r\n                    v-model=\"first_name\"\r\n                  />\r\n                </div>\r\n                <div class=\"field mb-4 col-12 md:col-6\">\r\n                  <label for=\"bio\" class=\"font-medium\">Last Name</label\r\n                  ><input\r\n                    class=\"p-inputtext p-component\"\r\n                    id=\"nickname\"\r\n                    type=\"text\"\r\n                    v-model=\"last_name\"\r\n                  />\r\n                </div>\r\n                <div class=\"field mb-4 col-12 md:col-6\">\r\n                  <label for=\"avatar\" class=\"font-medium\">Avatar</label>\r\n                  <div class=\"flex align-items-center\">\r\n                    <img\r\n                      src=\"images/blocks/avatars/circle/avatar-f-4.png\"\r\n                      class=\"mr-4\"\r\n                    />\r\n                    <div class=\"p-fileupload p-fileupload-basic p-component\">\r\n                      <span\r\n                        class=\"p-button p-component p-fileupload-choose p-button-outlined p-button-plain\"\r\n                        tabindex=\"0\"\r\n                        ><span\r\n                          class=\"p-button-icon p-button-icon-left pi pi-upload\"\r\n                        ></span\r\n                        ><span class=\"p-button-label\">Upload Image</span\r\n                        ><input type=\"file\" accept=\"image/*\" /><span\r\n                          class=\"p-ink\"\r\n                          role=\"presentation\"\r\n                        ></span\r\n                      ></span>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"field mb-4 col-12\">\r\n                  <label for=\"email\" class=\"font-medium\">Email</label\r\n                  ><input\r\n                    class=\"p-inputtext p-component\"\r\n                    id=\"email\"\r\n                    v-model=\"email\"\r\n                  />\r\n                </div>\r\n                <div class=\"col-12\">\r\n                  <button\r\n                    class=\"p-button p-component w-auto mt-3\"\r\n                    type=\"button\"\r\n                    aria-label=\"Save Changes\"\r\n                  >\r\n                    <!----><!----><span class=\"p-button-label\"\r\n                      >Save Changes</span\r\n                    ><!----><span class=\"p-ink\" role=\"presentation\"></span>\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <h2 class=\"p-m-2\">PROFILE INFORMATION</h2>\r\n    <hr style=\"height: 20px\" />\r\n    <Toast position=\"center\" />\r\n    <Dialog\r\n      header=\"Change Password\"\r\n      v-model:visible=\"display\"\r\n      style=\"height: 600px; width: 500px\"\r\n      modal=\"true\"\r\n    >\r\n      <div class=\"p-col-6 p-offset-3\" style=\"text-align: center\">\r\n        <h1>Old Password:</h1>\r\n        <Password v-model=\"value\" class=\"p-invalid\" />\r\n      </d